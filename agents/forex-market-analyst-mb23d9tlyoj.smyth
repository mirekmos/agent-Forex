.\nlet _output=undefined;\n","code_body":"// Formatowanie danych do wizualizacji\nlet formattedData = {\n  instrument: _instrument,\n  dataType: _data_type,\n  datasets: []\n};\n\n// Dodawanie różnych typów danych w zależności od wybranego typu\nswitch(_data_type.toLowerCase()) {\n  case 'sentiment':\n    if (_sentiment_data) {\n      formattedData.datasets.push({\n        type: 'sentiment',\n        title: 'Sentyment rynkowy',\n        data: _sentiment_data\n      });\n    }\n    break;\n  case 'technical':\n    if (_technical_data) {\n      formattedData.datasets.push({\n        type: 'technical',\n        title: 'Analiza techniczna',\n        data: _technical_data\n      });\n    }\n    break;\n  case 'news':\n    if (_news_data) {\n      formattedData.datasets.push({\n        type: 'news',\n        title: 'Analiza wiadomości',\n        data: _news_data\n      });\n    }\n    break;\n  case 'social':\n    if (_social_data) {\n      formattedData.datasets.push({\n        type: 'social',\n        title: 'Sentyment społeczny',\n        data: _social_data\n      });\n    }\n    break;\n  case 'calendar':\n    if (_calendar_data) {\n      formattedData.datasets.push({\n        type: 'calendar',\n        title: 'Kalendarz ekonomiczny',\n        data: _calendar_data\n      });\n    }\n    break;\n  case 'cot':\n    if (_cot_data) {\n      formattedData.datasets.push({\n        type: 'cot',\n        title: 'Commitment of Traders (COT)',\n        data: _cot_data\n      });\n    }\n    break;\n  case 'all':\n    // Dodaj wszystkie dostępne dane\n    if (_sentiment_data) {\n      formattedData.datasets.push({\n        type: 'sentiment',\n        title: 'Sentyment rynkowy',\n        data: _sentiment_data\n      });\n    }\n    if (_technical_data) {\n      formattedData.datasets.push({\n        type: 'technical',\n        title: 'Analiza techniczna',\n        data: _technical_data\n      });\n    }\n    if (_news_data) {\n      formattedData.datasets.push({\n        type: 'news',\n        title: 'Analiza wiadomości',\n        data: _news_data\n      });\n    }\n    if (_social_data) {\n      formattedData.datasets.push({\n        type: 'social',\n        title: 'Sentyment społeczny',\n        data: _social_data\n      });\n    }\n    if (_calendar_data) {\n      formattedData.datasets.push({\n        type: 'calendar',\n        title: 'Kalendarz ekonomiczny',\n        data: _calendar_data\n      });\n    }\n    if (_cot_data) {\n      formattedData.datasets.push({\n        type: 'cot',\n        title: 'Commitment of Traders (COT)',\n        data: _cot_data\n      });\n    }\n    break;\n  default:\n    formattedData.error = 'Nieznany typ danych';\n}\n\n_output = formattedData;"},"top":"4000px","left":"500px","width":"","height":"","displayName":"Code","title":"Data Formatter","aiTitle":"","description":"Formatuje dane do wizualizacji"},{"id":"CM76P5HCPQ9","name":"APICall","outputs":[{"name":"Response","color":"#3C89F9","index":0,"default":true},{"name":"Headers","color":"#3C89F9","index":1,"default":true}],"inputs":[{"name":"symbol","type":"String","friendlyInputType":"","coreInputType":"","color":"#F35063","optional":false,"index":0,"default":false}],"data":{"method":"GET","url":"http://localhost:5050/cot?symbol={{symbol}}","headers":"{}","contentType":"application/json","oauthService":"None","body":"","proxy":"","scope":"","authorizationURL":"","tokenURL":"","clientID":"","clientSecret":"","requestTokenURL":"","accessTokenURL":"","userAuthorizationURL":"","consumerKey":"","consumerSecret":"","authenticate":""},"top":"4300px","left":"500px","width":"","height":"","displayName":"APICall","title":"COT Data API","aiTitle":"","description":"Pobiera dane COT z API"},{"id":"CM76P5HCVZ9","name":"GenAILLM","outputs":[{"name":"Reply","color":"#3C89F9","index":0,"default":true},{"name":"visualization_html","color":"#3C89F9","index":1,"default":false}],"inputs":[{"name":"instrument","type":"String","friendlyInputType":"","coreInputType":"","color